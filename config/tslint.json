{
  "defaultSeverity": "error",
  "jsRules": {},
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "type-check": true,
    "exclude": [
      "./test/TestStarter",
      "../dist",
      "../node_modules"
    ]
  },
  "rules": {
    "eofline": false,
    "no-console": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "no-floating-promises": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "import-name": [
      false
    ],
    "quotemark": [
      true,
      "double",
      "avoid-escape",
      "avoid-template"
    ],
    "prefer-template": [
      true
    ]
  },
  "rulesDirectory": []
}